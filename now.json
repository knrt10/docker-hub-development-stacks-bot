{
  "name": "omp-bot",
  "version": 1,
  "alias": "omp-bot",
  "scale": {
    "all": {
      "min": 1,
      "max": 2
    }
  },
  "env": {
    "APP_ID": "@app-id-omp",
    "DISABLE_STATS": "true",
    "LOG_LEVEL": "fatal",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@private-key-omp",
    "WEBHOOK_SECRET": "@webhook-secret-omp"
  }
}
